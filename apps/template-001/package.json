{
  "name": "template-001",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve:dev:demo": "cross-env site_code=demo site_env=dev vite serve",
    "serve:dev:yb": "cross-env site_code=yb site_env=dev vite serve",
    "serve:dev:1xb": "cross-env site_code=1xb site_env=dev vite serve",
    "build:dev:demo": "cross-env site_code=demo site_env=dev vite build",
    "build:dev:yb": "cross-env site_code=yb site_env=dev vite build",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "scan": "i18next-scanner --config scripts/i18next-scaner.config.cjs",
    "i1n8Scan": "i18next-scanner --config src/i18n/i18next-scaner.config.js && node src/i18n/json2Excel.js",
    "i18n2Excel": "node scripts/json2Excel.cjs",
    "i18n2Json": "node scripts/excel2Json.cjs"
  },
  "dependencies": {
    "crc": "^4.3.2",
    "crypto-js": "^4.2.0",
    "js-md5": "^0.8.3",
    "pako": "^2.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@repo/eslint-conf": "workspace:^",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "fs": "0.0.1-security",
    "i18next-scanner": "^4.4.0",
    "json2xls": "^0.1.2",
    "node-xlsx": "^0.23.0",
    "npm-run-all2": "^6.1.2",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.5",
    "vue-tsc": "^2.0.6"
  }
}
